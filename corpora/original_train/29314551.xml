    <pair e1="29314551.s1.e0" e2="29314551.s1.e2" id="29314551.s1.p1" relation="true"/>
  </sentence>
  <sentence id="29314551.s2" text="skeletal muscle contraction plays a pivotal role in fetal muscle development and mutations in this gene are associated with Freeman-Sheldon syndrome, distal arthrogryposis 8 (DA8), and autosomal dominant spondylocarpotarsal synostosis.">
    <entity charOffset="0-27" id="29314551.s2.e0" ontology_id="GO_0003009" text="skeletal muscle contraction" type="GO"/>
    <entity charOffset="150-156" id="29314551.s2.e1" ontology_id="HP_0012839" text="distal" type="HP"/>
