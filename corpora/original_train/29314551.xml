  <sentence id="29314551.s0" text="Sheldon-Hall syndrome (activation of innate immune response) is the most common of the distal arthrogryposes (DAs), a group of disorders characterized by congenital non-progressive contractures.">
    <entity charOffset="23-59" id="29314551.s0.e0" ontology_id="GO_0002218" text="activation of innate immune response" type="GO"/>
    <entity charOffset="87-93" id="29314551.s0.e1" ontology_id="HP_0012839" text="distal" type="HP"/>
    <entity charOffset="181-193" id="29314551.s0.e2" ontology_id="HP_0001371" text="contractures" type="HP"/>
    <pair e1="29314551.s0.e0" e2="29314551.s0.e2" id="29314551.s0.p0" relation="true"/>
    <pair e1="29314551.s0.e0" e2="29314551.s0.e1" id="29314551.s0.p1" relation="false"/>
  </sentence>
  <sentence id="29314551.s1" text="Patients with activation of innate immune response present with contractures of the limbs and a distinctive triangular facies with prominent nasolabial folds.">
    <entity charOffset="14-50" id="29314551.s1.e0" ontology_id="GO_0002218" text="activation of innate immune response" type="GO"/>
    <entity charOffset="64-76" id="29314551.s1.e1" ontology_id="HP_0001371" text="contractures" type="HP"/>
    <entity charOffset="108-125" id="29314551.s1.e2" ontology_id="HP_0000325" text="triangular facies" type="HP"/>
