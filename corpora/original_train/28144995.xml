  <sentence id="28144995.s1" text="Among 510 unrelated patients with distal motor neuropathy, we identified mutations in regulation of protein phosphorylation (28 index patients/510; 5.5%) and HSPB8 (four index patients/510; 0.8%) genes.">
    <entity charOffset="34-40" id="28144995.s1.e0" ontology_id="HP_0012839" text="distal" type="HP"/>
