  </sentence>
  <sentence id="29335468.s1" text="Reciprocally, the cytosine methylation heterogeneity characteristic of normal GC B cells is lost upon somatic diversification of immunoglobulins depletion.">
    <entity charOffset="39-52" id="29335468.s1.e0" ontology_id="HP_0001425" text="heterogeneity" type="HP"/>
    <entity charOffset="102-144" id="29335468.s1.e1" ontology_id="GO_0016445" text="somatic diversification of immunoglobulins" type="GO"/>
