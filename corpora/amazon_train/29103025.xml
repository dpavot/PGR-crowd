  </sentence>
  <sentence id="29103025.s4" text="The diagnostic specificity of serum post-translational protein modification in cervical cancer occurrence was also compared with other high incidence diseases.">
    <entity charOffset="36-75" id="29103025.s4.e0" ontology_id="GO_0043687" text="post-translational protein modification" type="GO"/>
    <entity charOffset="88-94" id="29103025.s4.e1" ontology_id="HP_0002664" text="cancer" type="HP"/>
