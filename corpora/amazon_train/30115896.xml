  </sentence>
  <sentence id="30115896.s4" text="The enhanced chemotaxis expression in DCs is also noted in mice transplanted with colon cancer cells.">
    <entity charOffset="13-23" id="30115896.s4.e0" ontology_id="GO_0006935" text="chemotaxis" type="GO"/>
