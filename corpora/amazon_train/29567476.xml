  <sentence id="29567476.s0" text="Here, we report that mitotic spindle assembly checkpoint is highly overexpressed in multiple lung adenocarcinoma cell lines and tumor tissues.">
    <entity charOffset="21-56" id="29567476.s0.e0" ontology_id="GO_0007094" text="mitotic spindle assembly checkpoint" type="GO"/>
    <entity charOffset="93-112" id="29567476.s0.e1" ontology_id="HP_0030078" text="lung adenocarcinoma" type="HP"/>
    <pair e1="29567476.s0.e0" e2="29567476.s0.e1" id="29567476.s0.p0" relation="true"/>
  </sentence>
  <sentence id="29567476.s1" text="Clinically, mitotic spindle assembly checkpoint expression is positively associated with tumor size, T-stage, and N-stage, and Kaplan-Meier analysis revealed that heightened TRIP13 expression is associated with lower overall survival.">
    <entity charOffset="12-47" id="29567476.s1.e0" ontology_id="GO_0007094" text="mitotic spindle assembly checkpoint" type="GO"/>
    <entity charOffset="89-94" id="29567476.s1.e1" ontology_id="HP_0002664" text="tumor" type="HP"/>
    <pair e1="29567476.s1.e0" e2="29567476.s1.e1" id="29567476.s1.p0" relation="true"/>
  </sentence>
  <sentence id="29567476.s2" text="mitotic spindle assembly checkpoint promotes lung adenocarcinoma cell proliferation, clonogenicity, and migration while inhibiting apoptosis and G2/M phase shift in vitro.">
    <entity charOffset="0-35" id="29567476.s2.e0" ontology_id="GO_0007094" text="mitotic spindle assembly checkpoint" type="GO"/>
    <entity charOffset="45-64" id="29567476.s2.e1" ontology_id="HP_0030078" text="lung adenocarcinoma" type="HP"/>
    <pair e1="29567476.s2.e0" e2="29567476.s2.e1" id="29567476.s2.p0" relation="true"/>
  </sentence>
  <sentence id="29567476.s3" text="Accordingly, mitotic spindle assembly checkpoint-silenced xenograft tumors displayed significant growth inhibition in vivo.">
    <entity charOffset="13-48" id="29567476.s3.e0" ontology_id="GO_0007094" text="mitotic spindle assembly checkpoint" type="GO"/>
    <entity charOffset="68-73" id="29567476.s3.e1" ontology_id="HP_0002664" text="tumor" type="HP"/>
    <pair e1="29567476.s3.e0" e2="29567476.s3.e1" id="29567476.s3.p0" relation="true"/>
  </sentence>
  <sentence id="29567476.s4" text="In conclusion, heightened mitotic spindle assembly checkpoint expression appears to promote lung adenocarcinoma tumor progression and displays potential as a therapeutic target or biomarker for lung adenocarcinoma">
