    <pair e1="29864623.s0.e0" e2="29864623.s0.e2" id="29864623.s0.p1" relation="true"/>
  </sentence>
  <sentence id="29864623.s1" text="MicroRNA-214 (protein polyubiquitination-214) shows carcinogenesis in various tumor types, but little is known about biological functions of miR-214 in esophageal squamous cell carcinoma (ESCC).">
    <entity charOffset="14-40" id="29864623.s1.e0" ontology_id="GO_0000209" text="protein polyubiquitination" type="GO"/>
    <entity charOffset="78-83" id="29864623.s1.e1" ontology_id="HP_0002664" text="tumor" type="HP"/>
    <entity charOffset="163-186" id="29864623.s1.e2" ontology_id="HP_0002860" text="squamous cell carcinoma" type="HP"/>
    <pair e1="29864623.s1.e0" e2="29864623.s1.e1" id="29864623.s1.p0" relation="true"/>
    <pair e1="29864623.s1.e0" e2="29864623.s1.e2" id="29864623.s1.p1" relation="true"/>
  </sentence>
  <sentence id="29864623.s2" text="The results show protein polyubiquitination-214 serve as a tumor promoter regulating cells migration, invasion and apoptosis in ESCC.">
    <entity charOffset="17-43" id="29864623.s2.e0" ontology_id="GO_0000209" text="protein polyubiquitination" type="GO"/>
    <entity charOffset="59-64" id="29864623.s2.e1" ontology_id="HP_0002664" text="tumor" type="HP"/>
    <pair e1="29864623.s2.e0" e2="29864623.s2.e1" id="29864623.s2.p0" relation="true"/>
  </sentence>
  <sentence id="29864623.s3" text="In summary, these results suggest that protein polyubiquitination-214 serves as tumor promoter to promote proliferation, migration, invasion and inhibit apoptosis of ESCC cells by targeting negative regulation of macroautophagy via PI3K/AKT/mTOR signaling pathway.">
